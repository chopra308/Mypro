	<action-mappings>
<action path="/reg" name="Emp" validate="true" input="/index.jsp" >
			<forward name="success" path="/success.jsp"/>
		</action>
	</action-mappings>
	
	<message-resources parameter="beans/Messages"/>
</struts-config>
